        // Board state
        let currentPly = 0;
        let board = null;
        const startingFen = 'rnbqkbnr/pppppppp/8/8/8/8/PPPPPPPP/RNBQKBNR w KQkq - 0 1';

        // Wait for cm-chessboard to load
        function initBoard() {
            const boardEl = document.getElementById('chessboard');
            if (!boardEl || !window.Chessboard) {
                setTimeout(initBoard, 100);
                return;
            }

            board = new window.Chessboard(boardEl, {
                position: window.FEN.start,
                assetsUrl: "https://cdn.jsdelivr.net/npm/cm-chessboard@8/assets/",
                style: {
                    cssClass: "default",
                    showCoordinates: true,
                    borderType: "thin"
                },
                extensions: [{class: window.Markers}, {class: window.Arrows}]
            });

            updateDisplay();

            // Set up button handlers
            document.getElementById('btn-start').onclick = goToStart;
            document.getElementById('btn-prev').onclick = goToPrev;
            document.getElementById('btn-next').onclick = goToNext;
            document.getElementById('btn-end').onclick = goToEnd;
        }

        // Initialize when DOM is ready
        if (document.readyState === 'loading') {
            document.addEventListener('DOMContentLoaded', initBoard);
        } else {
            initBoard();
        }

        function selectPly(ply) {
            currentPly = ply;
            updateDisplay();
            // Scroll move table to show selected move
            const cell = document.getElementById('cell-' + ply);
            if (cell) {
                cell.scrollIntoView({ behavior: 'smooth', block: 'center' });
            }
        }
        function goToStart() {
            currentPly = 0;
            updateDisplay();
        }

        function goToPrev() {
            if (currentPly > 0) {
                // Find previous valid ply
                let prevPly = currentPly - 1;
                while (prevPly > 0 && !allMoves.find(m => m.ply === prevPly)) {
                    prevPly--;
                }
                if (prevPly >= 0) {
                    currentPly = prevPly > 0 ? prevPly : 0;
                    updateDisplay();
                }
            }
        }

        function goToNext() {
            const maxPly = allMoves.length > 0 ? Math.max(...allMoves.map(m => m.ply)) : 0;
            if (currentPly < maxPly) {
                // Find next valid ply
                let nextPly = currentPly + 1;
                while (nextPly < maxPly && !allMoves.find(m => m.ply === nextPly)) {
                    nextPly++;
                }
                if (nextPly <= maxPly) {
                    currentPly = nextPly;
                    updateDisplay();
                }
            }
        }

        function goToEnd() {
            if (allMoves.length > 0) {
                currentPly = Math.max(...allMoves.map(m => m.ply));
                updateDisplay();
            }
        }

        // Make functions globally accessible for onclick handlers
        window.selectPly = selectPly;
        window.goToStart = goToStart;
        window.goToPrev = goToPrev;
        window.goToNext = goToNext;
        window.goToEnd = goToEnd;

        // Helper to parse UCI move into from/to squares
        function parseUci(uci) {
            if (!uci || uci.length < 4) return null;
            return {
                from: uci.substring(0, 2),
                to: uci.substring(2, 4)
            };
        }

        // Draw arrows for played move, best move, and most human move
        function clearBoard() {
            if (!board) return;
            try { board.removeMarkers(); } catch(e) {}
            try { board.removeArrows(); } catch(e) {}
        }

        function drawMoveArrows(move) {
            if (!board || !move) return;

            const playedMove = parseUci(move.uci);
            const bestMove = parseUci(move.bestMoveUci);
            const topMaiaMove = parseUci(move.topMaiaMove);

            // Draw best move arrow (blue) - only if different from played
            if (bestMove && (!playedMove || bestMove.from !== playedMove.from || bestMove.to !== playedMove.to)) {
                try {
                    board.addArrow(window.ARROW_TYPE.default, bestMove.from, bestMove.to);
                } catch(e) { console.log('Arrow error:', e); }
            }

            // Draw most human move (info/secondary color) - only if different from played and best
            if (topMaiaMove &&
                (!playedMove || topMaiaMove.from !== playedMove.from || topMaiaMove.to !== playedMove.to) &&
                (!bestMove || topMaiaMove.from !== bestMove.from || topMaiaMove.to !== bestMove.to)) {
                try {
                    board.addArrow(window.ARROW_TYPE.info, topMaiaMove.from, topMaiaMove.to);
                } catch(e) { console.log('Maia arrow error:', e); }
            }

            // Draw played move - highlight from/to squares (green)
            if (playedMove) {
                try {
                    board.addArrow(window.ARROW_TYPE.success, playedMove.from, playedMove.to);
                } catch(e) { console.log('Played arrow error:', e); }
            }
        }

        // Preview an alternative move on the board
        let previewingMove = null;
        function previewMove(uci) {
            if (!board) return;
            clearBoard();

            const parsed = parseUci(uci);
            if (parsed) {
                previewingMove = uci;
                try {
                    board.addArrow(window.ARROW_TYPE.danger, parsed.from, parsed.to);
                } catch(e) {}
            }
        }

        function clearPreview() {
            if (!board) return;
            previewingMove = null;
            const move = allMoves.find(m => m.ply === currentPly);
            if (move) {
                clearBoard();
                drawMoveArrows(move);
            }
        }

        // Make preview functions global
        window.previewMove = previewMove;
        window.clearPreview = clearPreview;

        function updateDisplay() {
            // Update ply indicator
            const indicator = document.getElementById('ply-indicator');
            if (currentPly === 0) {
                indicator.textContent = 'Start Position';
            } else {
                const move = allMoves.find(m => m.ply === currentPly);
                if (move) {
                    indicator.textContent = `Move ${move.moveNum}${move.player === 'white' ? '.' : '...'} ${move.san}`;
                }
            }

            // Update board position and arrows
            if (board) {
                clearBoard();

                if (currentPly === 0) {
                    board.setPosition(window.FEN.start, true);
                } else {
                    const move = allMoves.find(m => m.ply === currentPly);
                    if (move && move.fenBefore) {
                        // Show position BEFORE the move so arrows make sense
                        board.setPosition(move.fenBefore, true);

                        // Draw arrows after position is set
                        setTimeout(() => {
                            drawMoveArrows(move);
                        }, 100);
                    } else if (move && move.fen) {
                        board.setPosition(move.fen, true);
                    }
                }
            }

            // Update navigation buttons
            const maxPly = allMoves.length > 0 ? Math.max(...allMoves.map(m => m.ply)) : 0;
            document.getElementById('btn-start').disabled = currentPly === 0;
            document.getElementById('btn-prev').disabled = currentPly === 0;
            document.getElementById('btn-next').disabled = currentPly >= maxPly;
            document.getElementById('btn-end').disabled = currentPly >= maxPly;

            // Update move table highlighting
            document.querySelectorAll('.move-table-cell.selected').forEach(el => {
                el.classList.remove('selected');
            });
            if (currentPly > 0) {
                const cell = document.getElementById('cell-' + currentPly);
                if (cell) {
                    cell.classList.add('selected');
                }
            }

            // Update current move display
            updateMoveDetails();
        }

        function updateMoveDetails() {
            const sanEl = document.getElementById('current-san');
            const playerEl = document.getElementById('current-player');
            const badgesEl = document.getElementById('current-badges');
            const metricsEl = document.getElementById('current-metrics');

            if (currentPly === 0) {
                sanEl.textContent = '-';
                playerEl.textContent = '';
                badgesEl.innerHTML = '';
                metricsEl.innerHTML = '<p style="color: #666; font-style: italic;">Select a move to view details</p>';
                return;
            }

            const move = allMoves.find(m => m.ply === currentPly);
            if (!move) return;

            // Update SAN and player
            sanEl.textContent = move.san;
            playerEl.textContent = `by ${move.playerName} (${move.player === 'white' ? 'White' : 'Black'})`;

            // Update badges
            let badges = '';
            if (move.isBook) badges += '<span class="move-badge badge-book">Book</span>';
            if (move.moveClass === 'Blunder') badges += '<span class="move-badge badge-blunder">Blunder</span>';
            else if (move.moveClass === 'Mistake') badges += '<span class="move-badge badge-mistake">Mistake</span>';
            else if (move.moveClass === 'Inaccuracy') badges += '<span class="move-badge badge-inaccuracy">Inaccuracy</span>';
            else if (move.moveClass === 'Best') badges += '<span class="move-badge badge-best">Best</span>';
            else if (move.moveClass === 'Excellent') badges += '<span class="move-badge badge-good">Excellent</span>';
            else if (move.moveClass === 'Good') badges += '<span class="move-badge badge-good">Good</span>';
            if (move.humanness !== null && move.humanness < 1) badges += '<span class="move-badge badge-low-human">Low Human</span>';
            if (move.complexityCategory === 'VERY_HIGH' || move.complexityCategory === 'HIGH') badges += '<span class="move-badge badge-complex">' + move.complexityCategory + '</span>';
            badgesEl.innerHTML = badges;

            // Build candidate moves chart + list for Stockfish
            let sfChartHtml = '';
            let sfCandidatesHtml = '';
            if (move.candidateMoves && move.candidateMoves.length > 0) {
                sfChartHtml = `<div class="sf-eval-chart-container"><canvas id="sfEvalChart"></canvas></div>`;
                sfCandidatesHtml = `
                    <details class="candidate-moves">
                        <summary>All Candidate Moves (${move.candidateMoves.length})</summary>
                        <table class="candidate-table">
                            <tr><th>#</th><th>Move</th><th>Eval</th></tr>
                            ${move.candidateMoves.map(cm => `
                                <tr class="${cm.is_played ? 'played-move' : ''}"
                                    onmouseenter="previewMove('${cm.uci}')"
                                    onmouseleave="clearPreview()"
                                    onclick="previewMove('${cm.uci}')">
                                    <td>${cm.rank}</td>
                                    <td>${cm.san}${cm.is_played ? ' ✓' : ''}</td>
                                    <td>${cm.eval >= 0 ? '+' : ''}${cm.eval.toFixed(2)}</td>
                                </tr>
                            `).join('')}
                        </table>
                    </details>`;
            }

            // Build candidate moves list for Maia2
            let maiaCandidatesHtml = '';
            if (move.maiaMoves && move.maiaMoves.length > 0) {
                maiaCandidatesHtml = `
                    <details class="candidate-moves">
                        <summary>All Human Moves (${move.maiaMoves.length})</summary>
                        <table class="candidate-table">
                            <tr><th>#</th><th>Move</th><th>Prob</th></tr>
                            ${move.maiaMoves.map(mm => `
                                <tr class="${mm.is_played ? 'played-move' : ''}"
                                    onmouseenter="previewMove('${mm.uci}')"
                                    onmouseleave="clearPreview()"
                                    onclick="previewMove('${mm.uci}')">
                                    <td>${mm.rank}</td>
                                    <td>${mm.san}${mm.is_played ? ' ✓' : ''}</td>
                                    <td>${mm.prob.toFixed(1)}%</td>
                                </tr>
                            `).join('')}
                        </table>
                    </details>`;
            }

            // Update metrics
            let html = `
                <div class="metrics-section">
                    <h6 class="metrics-section-title">Stockfish</h6>
                    <table class="metrics-table">
                        <tr><td>Eval:</td><td>${move.evalBefore !== null ? move.evalBefore.toFixed(2) + ' → ' + (move.evalAfter !== null ? move.evalAfter.toFixed(2) : move.evalBefore.toFixed(2)) + (move.depth ? ' (d' + move.depth + ')' : '') : 'N/A'}</td></tr>
                        <tr><td>CPL:</td><td>${move.cpl.toFixed(0)}${move.moveClass ? ' (' + move.moveClass + ')' : ''}</td></tr>
                        <tr><td>Move Rank:</td><td>${move.moveRank !== null ? '#' + move.moveRank + ' of ' + (move.totalLegalMoves || '?') : 'N/A'}</td></tr>
                    </table>
                    ${sfChartHtml}
                    ${sfCandidatesHtml}
                </div>
                <div class="metrics-section">
                    <h6 class="metrics-section-title">Maia2</h6>
                    <table class="metrics-table">
                        <tr><td>Humanness:</td><td>${move.humanness !== null ? move.humanness.toFixed(1) + '% (Rank #' + (move.maiaRank || '?') + ')' : 'N/A'}</td></tr>
                        <tr><td>Human Moves:</td><td>${move.numHumanMoves !== null ? move.numHumanMoves + ' (≥1% prob)' : 'N/A'}</td></tr>
                        <tr><td>Expected:</td><td>${move.topMaiaMove ? move.topMaiaMove + ' (' + (move.topMaiaProb !== null ? move.topMaiaProb.toFixed(1) + '%' : '?') + ')' : 'N/A'}</td></tr>
                        <tr><td>CP Adjust:</td><td>${move.cpAdjustment !== null ? (move.cpAdjustment >= 0 ? '+' : '') + move.cpAdjustment.toFixed(0) + 'cp' : 'N/A'}</td></tr>
                    </table>
                    ${maiaCandidatesHtml}
                </div>
                <div class="metrics-section">
                    <h6 class="metrics-section-title">Position</h6>
                    <table class="metrics-table">
                        <tr><td>Phase:</td><td>${move.gamePhase ? move.gamePhase.charAt(0).toUpperCase() + move.gamePhase.slice(1) : 'N/A'}</td></tr>
                        <tr><td>Material:</td><td>${move.pureMaterial !== null ? (move.pureMaterial >= 0 ? '+' : '') + move.pureMaterial : 'N/A'}</td></tr>
                        <tr><td>Win %:</td><td>${move.winPctBefore !== null ? move.winPctBefore.toFixed(1) + '% → ' + (move.winPctAfter !== null ? move.winPctAfter.toFixed(1) + '%' : move.winPctBefore.toFixed(1) + '%') : 'N/A'}</td></tr>
                        <tr><td>Fragility:</td><td>${move.fragility.toFixed(2)}${move.fragilityTrend === 'increasing' ? ' ↑' : move.fragilityTrend === 'decreasing' ? ' ↓' : move.fragilityTrend === 'stable' ? ' →' : ''}</td></tr>
                        <tr><td>Complexity:</td><td>${move.complexityScore !== null ? (move.complexityScore * 100).toFixed(0) + '% (' + move.complexityCategory + ')' : 'N/A'}</td></tr>
                        <tr><td>Book Status:</td><td>${move.isBook ? 'In Book' : move.distanceFromBook !== null ? move.distanceFromBook + ' plys from book' : 'N/A'}</td></tr>
                    </table>
                </div>`;
            metricsEl.innerHTML = html;

            // Render Stockfish eval chart
            renderSfEvalChart(move);
        }

        // Track current chart instance so we can destroy it on navigation
        let sfEvalChartInstance = null;

        function renderSfEvalChart(move) {
            if (!move.candidateMoves || move.candidateMoves.length === 0) return;
            const canvas = document.getElementById('sfEvalChart');
            if (!canvas) return;

            if (sfEvalChartInstance) {
                sfEvalChartInstance.destroy();
                sfEvalChartInstance = null;
            }

            // Moves are already sorted best-to-worst by rank
            const candidates = move.candidateMoves;
            const bestEval = candidates[0].eval;
            const isWhite = move.player === 'white';

            // CPL as negative values (0 at top, worst at bottom)
            // Evals are absolute (positive = white advantage), so for black
            // a lower eval is better — flip the subtraction direction.
            const cpls = candidates.map(cm => {
                const loss = isWhite
                    ? (bestEval - cm.eval) * 100
                    : (cm.eval - bestEval) * 100;
                return -Math.round(loss);
            });

            // Classification colors (matching the badges)
            function cplColor(negCpl) {
                const cpl = Math.abs(negCpl);
                if (cpl < 10) return '#1565c0';    // Best/Excellent - blue
                if (cpl < 25) return '#2e7d32';     // Good - green
                if (cpl < 50) return '#f5a623';     // Inaccuracy - amber
                if (cpl < 100) return '#e67700';    // Mistake - orange
                return '#c43432';                    // Blunder - red
            }

            const labels = candidates.map(cm => cm.san);
            const playedIdx = candidates.findIndex(cm => cm.is_played);

            // Build background color bands (negative Y axis, high contrast)
            const maxCpl = Math.max(...cpls.map(c => Math.abs(c)), 10);
            const bandDefs = [
                { yMin: 0,    yMax: -Math.min(10, maxCpl),  color: 'rgba(21,101,192,0.25)',  label: 'Best' },
                { yMin: -10,  yMax: -Math.min(25, maxCpl),  color: 'rgba(46,125,50,0.25)',   label: 'Good' },
                { yMin: -25,  yMax: -Math.min(50, maxCpl),  color: 'rgba(245,166,35,0.25)',  label: 'Inaccuracy' },
                { yMin: -50,  yMax: -Math.min(100, maxCpl), color: 'rgba(230,119,0,0.25)',   label: 'Mistake' },
                { yMin: -100, yMax: -maxCpl,                color: 'rgba(196,52,50,0.25)',   label: 'Blunder' },
            ].filter(b => Math.abs(b.yMin) < maxCpl);

            const annotations = {};
            bandDefs.forEach((band, i) => {
                annotations['band' + i] = {
                    type: 'box',
                    yMin: band.yMin,
                    yMax: band.yMax,
                    backgroundColor: band.color,
                    borderWidth: 0,
                    drawTime: 'beforeDatasetsDraw',
                };
                annotations['bandLabel' + i] = {
                    type: 'label',
                    xValue: labels.length - 1,
                    yValue: (band.yMin + band.yMax) / 2,
                    content: band.label,
                    font: { size: 9, weight: 'normal' },
                    color: '#666',
                    position: { x: 'end' },
                };
            });

            // Played move vertical line
            if (playedIdx >= 0) {
                annotations['playedLine'] = {
                    type: 'line',
                    xMin: playedIdx,
                    xMax: playedIdx,
                    borderColor: 'rgba(21,120,27,0.5)',
                    borderWidth: 2,
                    borderDash: [4, 4],
                    drawTime: 'beforeDatasetsDraw',
                };
            }

            sfEvalChartInstance = new Chart(canvas, {
                type: 'line',
                data: {
                    labels: labels,
                    datasets: [{
                        data: cpls,
                        borderColor: '#555',
                        borderWidth: 1.5,
                        pointRadius: cpls.map((_, i) => i === playedIdx ? 6 : 3),
                        pointBackgroundColor: cpls.map((c, i) => i === playedIdx ? '#15781B' : cplColor(c)),
                        pointBorderColor: cpls.map((c, i) => i === playedIdx ? '#0d5012' : cplColor(c)),
                        pointBorderWidth: cpls.map((_, i) => i === playedIdx ? 2 : 1),
                        pointStyle: cpls.map((_, i) => i === playedIdx ? 'rectRot' : 'circle'),
                        fill: false,
                        tension: 0.3,
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    plugins: {
                        legend: { display: false },
                        tooltip: {
                            callbacks: {
                                title: (items) => {
                                    const idx = items[0].dataIndex;
                                    const cm = candidates[idx];
                                    return cm.san + (cm.is_played ? ' (played)' : '');
                                },
                                label: (item) => {
                                    const idx = item.dataIndex;
                                    const cm = candidates[idx];
                                    const cpl = cpls[idx];
                                    return [
                                        `Eval: ${cm.eval >= 0 ? '+' : ''}${cm.eval.toFixed(2)}`,
                                        `CPL: ${cpl}`,
                                    ];
                                }
                            }
                        },
                        annotation: {
                            annotations: annotations
                        }
                    },
                    scales: {
                        x: {
                            ticks: {
                                font: { size: 9 },
                                maxRotation: 90,
                                minRotation: 45,
                                autoSkip: true,
                                maxTicksLimit: 20,
                            },
                            grid: { display: false },
                            title: {
                                display: true,
                                text: 'Moves (best \u2192 worst)',
                                font: { size: 10 },
                                color: '#888',
                            }
                        },
                        y: {
                            max: 0,
                            min: -maxCpl,
                            title: {
                                display: true,
                                text: 'CPL',
                                font: { size: 10 },
                                color: '#888',
                            },
                            ticks: { font: { size: 9 } },
                        }
                    },
                    onClick: (evt, elements) => {
                        if (elements.length > 0) {
                            const idx = elements[0].index;
                            const cm = candidates[idx];
                            previewMove(cm.uci);
                        }
                    }
                }
            });
        }
    </script>
</body>
</html>
