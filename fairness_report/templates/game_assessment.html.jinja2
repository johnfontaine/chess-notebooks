    <script>
        function toggleDetails(id) {
            const el = document.getElementById(id);
            if (el) {
                el.classList.toggle('active');
            }
        }

        // Chart data from template
        const chartData = {{ chart_data | tojson }};
        const phaseTransitions = {{ phase_transitions | tojson }};
        const phasePlyCounts = {{ phase_ply_counts | default({}) | tojson }};
        const totalMoves = phasePlyCounts.total_moves || chartData.moves.length;

        // Phase annotation boxes and lines
        function getPhaseAnnotations() {
            const annotations = {};

            // Calculate phase boundaries (in move numbers)
            const bookEnd = phaseTransitions.book_end || 0;
            const openingEnd = phaseTransitions.opening_end || 0;
            const middlegameEnd = phaseTransitions.middlegame_end || totalMoves;

            // Opening phase box (blue) - from start to opening end
            if (openingEnd > 0) {
                annotations['openingBox'] = {
                    type: 'box',
                    xMin: 0,
                    xMax: openingEnd,
                    backgroundColor: 'rgba(52, 152, 219, 0.1)',
                    borderWidth: 0,
                };
            }

            // Middlegame phase box (green) - from opening end to middlegame end
            if (openingEnd > 0 && middlegameEnd > openingEnd) {
                annotations['middlegameBox'] = {
                    type: 'box',
                    xMin: openingEnd,
                    xMax: middlegameEnd,
                    backgroundColor: 'rgba(39, 174, 96, 0.1)',
                    borderWidth: 0,
                };
            }

            // Endgame phase box (purple) - from middlegame end to end of game
            if (phaseTransitions.middlegame_end && middlegameEnd < totalMoves) {
                annotations['endgameBox'] = {
                    type: 'box',
                    xMin: middlegameEnd,
                    xMax: totalMoves,
                    backgroundColor: 'rgba(155, 89, 182, 0.1)',
                    borderWidth: 0,
                };
            }

            // Phase labels as box annotations at the top (outside the chart area)
            // Opening label
            if (openingEnd > 0) {
                annotations['openingLabel'] = {
                    type: 'label',
                    xValue: openingEnd / 2,
                    yValue: 'max',
                    yAdjust: -10,
                    content: 'Opening',
                    backgroundColor: 'rgba(52, 152, 219, 0.8)',
                    color: 'white',
                    font: { size: 10, weight: 'bold' },
                    padding: { top: 2, bottom: 2, left: 6, right: 6 },
                    borderRadius: 3,
                };
            }

            // Middlegame label
            if (openingEnd > 0 && middlegameEnd > openingEnd) {
                const midpoint = openingEnd + (middlegameEnd - openingEnd) / 2;
                annotations['middlegameLabel'] = {
                    type: 'label',
                    xValue: midpoint,
                    yValue: 'max',
                    yAdjust: -10,
                    content: 'Middlegame',
                    backgroundColor: 'rgba(39, 174, 96, 0.8)',
                    color: 'white',
                    font: { size: 10, weight: 'bold' },
                    padding: { top: 2, bottom: 2, left: 6, right: 6 },
                    borderRadius: 3,
                };
            }

            // Endgame label
            if (phaseTransitions.middlegame_end && middlegameEnd < totalMoves) {
                const midpoint = middlegameEnd + (totalMoves - middlegameEnd) / 2;
                annotations['endgameLabel'] = {
                    type: 'label',
                    xValue: midpoint,
                    yValue: 'max',
                    yAdjust: -10,
                    content: 'Endgame',
                    backgroundColor: 'rgba(155, 89, 182, 0.8)',
                    color: 'white',
                    font: { size: 10, weight: 'bold' },
                    padding: { top: 2, bottom: 2, left: 6, right: 6 },
                    borderRadius: 3,
                };
            }

            // Vertical transition lines (no labels, just lines)
            if (phaseTransitions.book_end) {
                annotations['bookLine'] = {
                    type: 'line',
                    xMin: phaseTransitions.book_end,
                    xMax: phaseTransitions.book_end,
                    borderColor: 'rgba(128, 0, 128, 0.5)',
                    borderWidth: 1,
                    borderDash: [3, 3],
                };
            }
            if (phaseTransitions.opening_end) {
                annotations['openingLine'] = {
                    type: 'line',
                    xMin: phaseTransitions.opening_end,
                    xMax: phaseTransitions.opening_end,
                    borderColor: 'rgba(52, 152, 219, 0.5)',
                    borderWidth: 1,
                    borderDash: [3, 3],
                };
            }
            if (phaseTransitions.middlegame_end) {
                annotations['middlegameLine'] = {
                    type: 'line',
                    xMin: phaseTransitions.middlegame_end,
                    xMax: phaseTransitions.middlegame_end,
                    borderColor: 'rgba(39, 174, 96, 0.5)',
                    borderWidth: 1,
                    borderDash: [3, 3],
                };
            }
            return annotations;
        }

        // Common chart options
        const commonOptions = {
            responsive: true,
            maintainAspectRatio: false,
            interaction: { mode: 'index', intersect: false },
            plugins: {
                annotation: { annotations: getPhaseAnnotations() },
                legend: { position: 'bottom', labels: { boxWidth: 12, padding: 8 } }
            },
            scales: {
                x: {
                    title: { display: true, text: 'Move #' },
                    ticks: { maxTicksLimit: 20 }
                }
            }
        };

        // Player names for chart labels
        const player1 = '{{ game.white }}';
        const player2 = '{{ game.black }}';

        // 1. Win Probability Chart
        new Chart(document.getElementById('winProbChart'), {
            type: 'line',
            data: {
                labels: chartData.moves,
                datasets: [
                    {
                        label: player1 + ' Win % (Lichess)',
                        data: chartData.white_win_pct,
                        borderColor: '#3498db',
                        backgroundColor: 'rgba(46, 204, 113, 0.2)',
                        borderWidth: 2,
                        pointRadius: 0,
                        tension: 0.1,
                        fill: false
                    },
                    {
                        label: player2 + ' Win % (Lichess)',
                        data: chartData.black_win_pct,
                        borderColor: '#e67e22',
                        backgroundColor: 'rgba(231, 76, 60, 0.2)',
                        borderWidth: 2,
                        pointRadius: 0,
                        tension: 0.1,
                        fill: false
                    },
                    {
                        label: player1 + ' SF Win %',
                        data: chartData.white_wdl_win,
                        borderColor: '#2980b9',
                        borderWidth: 2,
                        borderDash: [5, 5],
                        pointRadius: 0,
                        tension: 0.1,
                        fill: false
                    },
                    {
                        label: player2 + ' SF Win %',
                        data: chartData.black_wdl_win,
                        borderColor: '#d35400',
                        borderWidth: 2,
                        borderDash: [5, 5],
                        pointRadius: 0,
                        tension: 0.1,
                        fill: false
                    },
                    {
                        label: 'SF Draw %',
                        data: chartData.white_wdl_draw,
                        borderColor: '#7f8c8d',
                        borderWidth: 2,
                        borderDash: [2, 2],
                        pointRadius: 0,
                        tension: 0.1,
                        fill: false
                    }
                ]
            },
            options: {
                ...commonOptions,
                scales: {
                    ...commonOptions.scales,
                    y: { min: 0, max: 100, title: { display: true, text: 'Win %' } }
                }
            }
        });

        // 2. Accuracy Chart
        new Chart(document.getElementById('accuracyChart'), {
            type: 'line',
            data: {
                labels: chartData.moves,
                datasets: [
                    {
                        label: player1 + ' Accuracy',
                        data: chartData.white_accuracy,
                        borderColor: '#3498db',
                        backgroundColor: 'rgba(46, 204, 113, 0.5)',
                        borderWidth: 2,
                        pointRadius: 3,
                        spanGaps: true
                    },
                    {
                        label: player2 + ' Accuracy',
                        data: chartData.black_accuracy,
                        borderColor: '#e67e22',
                        backgroundColor: 'rgba(231, 76, 60, 0.5)',
                        borderWidth: 2,
                        pointRadius: 3,
                        spanGaps: true
                    }
                ]
            },
            options: {
                ...commonOptions,
                scales: {
                    ...commonOptions.scales,
                    y: { min: 0, max: 100, title: { display: true, text: 'Accuracy %' } }
                }
            }
        });

        // 3. Humanness Chart
        new Chart(document.getElementById('humannessChart'), {
            type: 'line',
            data: {
                labels: chartData.moves,
                datasets: [
                    {
                        label: player1 + ' Humanness',
                        data: chartData.white_humanness,
                        borderColor: '#3498db',
                        backgroundColor: 'rgba(46, 204, 113, 0.5)',
                        borderWidth: 2,
                        pointRadius: 3,
                        spanGaps: true
                    },
                    {
                        label: player2 + ' Humanness',
                        data: chartData.black_humanness,
                        borderColor: '#e67e22',
                        backgroundColor: 'rgba(231, 76, 60, 0.5)',
                        borderWidth: 2,
                        pointRadius: 3,
                        spanGaps: true
                    }
                ]
            },
            options: {
                ...commonOptions,
                scales: {
                    ...commonOptions.scales,
                    y: { min: 0, max: 100, title: { display: true, text: 'Humanness %' } }
                }
            }
        });

        // 4. Fragility & Complexity Chart (dual Y-axis)
        new Chart(document.getElementById('fragilityChart'), {
            type: 'line',
            data: {
                labels: chartData.moves,
                datasets: [
                    {
                        label: 'Fragility',
                        data: chartData.fragility,
                        borderColor: 'rgba(52, 152, 219, 0.9)',
                        backgroundColor: 'rgba(52, 152, 219, 0.2)',
                        borderWidth: 2,
                        pointRadius: 2,
                        yAxisID: 'y',
                        tension: 0.1
                    },
                    {
                        label: 'Complexity',
                        data: chartData.complexity,
                        borderColor: 'rgba(231, 76, 60, 0.8)',
                        backgroundColor: 'rgba(231, 76, 60, 0.2)',
                        borderWidth: 2,
                        pointRadius: 0,
                        yAxisID: 'y1',
                        fill: true,
                        tension: 0.1
                    }
                ]
            },
            options: {
                ...commonOptions,
                scales: {
                    ...commonOptions.scales,
                    y: {
                        type: 'linear',
                        display: true,
                        position: 'left',
                        min: 0,
                        title: { display: true, text: 'Fragility' }
                    },
                    y1: {
                        type: 'linear',
                        display: true,
                        position: 'right',
                        min: 0,
                        max: 100,
                        title: { display: true, text: 'Complexity %' },
                        grid: { drawOnChartArea: false }
                    }
                }
            }
        });

        // Distribution chart helper function
        function createDistributionChart(canvasId, labels, data, color, chartType = 'bar') {
            const config = {
                type: chartType,
                data: {
                    labels: labels,
                    datasets: [{
                        data: data,
                        backgroundColor: color,
                        borderColor: color.replace('0.7', '1'),
                        borderWidth: chartType === 'line' ? 2 : 1,
                        fill: chartType === 'line',
                        tension: 0.4,
                        pointRadius: chartType === 'line' ? 3 : 0
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    plugins: {
                        legend: { display: false },
                        tooltip: {
                            callbacks: {
                                label: function(ctx) {
                                    return ctx.raw + ' moves';
                                }
                            }
                        }
                    },
                    scales: {
                        y: {
                            beginAtZero: true,
                            ticks: { stepSize: 1 }
                        },
                        x: {
                            ticks: { font: { size: 10 } }
                        }
                    }
                }
            };
            new Chart(document.getElementById(canvasId), config);
        }

        // White player distribution charts
        const whiteCplHist = {{ white_summary.cpl_histogram | tojson }};
        const whiteAccHist = {{ white_summary.accuracy_histogram | tojson }};
        const whiteHumanHist = {{ white_summary.humanness_histogram | tojson }};

        createDistributionChart('whiteCplDist', whiteCplHist.labels, whiteCplHist.counts, 'rgba(52, 152, 219, 0.3)', 'line');
        createDistributionChart('whiteAccDist', whiteAccHist.labels, whiteAccHist.counts, 'rgba(46, 204, 113, 0.7)', 'bar');
        createDistributionChart('whiteHumanDist', whiteHumanHist.labels, whiteHumanHist.counts, 'rgba(155, 89, 182, 0.7)', 'bar');

        // Black player distribution charts
        const blackCplHist = {{ black_summary.cpl_histogram | tojson }};
        const blackAccHist = {{ black_summary.accuracy_histogram | tojson }};
        const blackHumanHist = {{ black_summary.humanness_histogram | tojson }};

        createDistributionChart('blackCplDist', blackCplHist.labels, blackCplHist.counts, 'rgba(230, 126, 34, 0.3)', 'line');
        createDistributionChart('blackAccDist', blackAccHist.labels, blackAccHist.counts, 'rgba(46, 204, 113, 0.7)', 'bar');
        createDistributionChart('blackHumanDist', blackHumanHist.labels, blackHumanHist.counts, 'rgba(155, 89, 182, 0.7)', 'bar');
    </script>

    <!-- Glossary Section -->
    <section class="glossary-section">
        <h2>Definitions & Glossary</h2>

        <div class="glossary-columns">
            <div>
                <h3>Stockfish Metrics</h3>
                <dl class="glossary">
                    <dt>CPL (Centipawn Loss)</dt>
                    <dd>Difference between the best move evaluation and the played move evaluation, measured in centipawns (1/100th of a pawn). Lower values indicate more accurate play. 0 = best move was played.</dd>

                    <dt>Move Classification</dt>
                    <dd>Move quality categorization: <strong>Best</strong> (0 CPL), <strong>Excellent</strong> (1-9 CPL), <strong>Good</strong> (10-24 CPL), <strong>Inaccuracy</strong> (25-49 CPL), <strong>Mistake</strong> (50-299 CPL), <strong>Blunder</strong> (300+ CPL).</dd>

                    <dt>Move Accuracy</dt>
                    <dd>Individual move quality score (0-100%) using the Lichess formula based on win percentage preservation. A move that maintains or improves the position scores near 100%.</dd>

                    <dt>Win % (Lichess)</dt>
                    <dd>Expected probability of winning, calculated from centipawn evaluation. Formula: 50 + 50 * (2 / (1 + exp(-0.00368208 * cp)) - 1). Equal position (0cp) = 50%.</dd>

                    <dt>WDL (SF)</dt>
                    <dd>Stockfish's Win/Draw/Loss percentages from neural network evaluation. Format: W%/D%/L% before → after. More reliable than centipawn conversion in positions where evaluation is unstable.</dd>

                    <dt>Move Rank</dt>
                    <dd>Position of the played move among all legal moves sorted by engine evaluation. Rank #1 means the best move was played.</dd>

                    <dt>Gap to 2nd Best</dt>
                    <dd>Centipawn difference between the engine's best move and second-best move. Small gaps (&lt;50cp) indicate positions with multiple viable moves.</dd>

                    <dt>Playable Moves</dt>
                    <dd>Count of legal moves within 50 centipawns of the best move. More playable moves = more forgiving position.</dd>

                    <dt>Eval Volatility</dt>
                    <dd>Standard deviation of centipawn evaluations across multiple analysis depths. Higher volatility indicates unstable positions where evaluations change significantly with deeper analysis.</dd>
                </dl>

                <h3>Branching Factors</h3>
                <dl class="glossary">
                    <dt>SF Branching Factor</dt>
                    <dd>Engine-derived branching factor from Stockfish node counts. Calculated as (nodes_d2/nodes_d1)^(1/(d2-d1)) averaged across depth pairs. Typical values: 1.5-4. Measures engine search tree expansion.</dd>

                    <dt>Raw Branching Factor</dt>
                    <dd>True positional branching calculated by counting all legal moves at 3-ply depth (your moves, opponent's responses, your replies). Typical values: ~20-25 for openings, ~5-10 for endgames.</dd>
                </dl>
            </div>

            <div>
                <h3>Maia2 Metrics</h3>
                <dl class="glossary">
                    <dt>Humanness</dt>
                    <dd>Probability that a human at the player's rating level would play this move, according to the Maia2 neural network. Higher values indicate more human-like move choices. Moves with &lt;1% probability are flagged as "Low Human" since they fall outside the typical distribution of human play.</dd>

                    <dt>Maia Rank</dt>
                    <dd>Rank of the played move among Maia2's predictions. Rank #1 = most human-like move for this rating level.</dd>

                    <dt>Num Human Moves</dt>
                    <dd>Count of moves with &ge;1% probability according to Maia2. More human moves indicate positions where typical players consider multiple options.</dd>

                    <dt>CP Adjustment</dt>
                    <dd>Centipawn difference between played move and Maia2's top predicted move. Negative values mean the player found a stronger move than the most human-like choice.</dd>
                </dl>

                <h3>Position Assessment</h3>
                <dl class="glossary">
                    <dt>Game Phase</dt>
                    <dd>Position classification (opening/middlegame/endgame) based on piece count and development.</dd>

                    <dt>Pure Material</dt>
                    <dd>Material balance using standard values: P=1, N=3, B=3, R=5, Q=9. Positive = White advantage.</dd>

                    <dt>Fragility</dt>
                    <dd>Graph-theoretic measure of position vulnerability. High fragility means small errors can have large consequences. Arrows indicate trend: ↑ increasing, ↓ decreasing, → stable.</dd>

                    <dt>Complexity Score</dt>
                    <dd>Composite position difficulty (0-100%) combining evaluation volatility, gap to 2nd best, convergence depth, and branching factor. Categories: LOW (&lt;25%), MEDIUM (25-50%), HIGH (50-75%), VERY_HIGH (&gt;75%).</dd>

                    <dt>Distance from Book</dt>
                    <dd>Number of half-moves (plys) since the last opening book position. "In Book" means the position is a known opening line.</dd>

                    <dt>Tablebase Status</dt>
                    <dd>For positions with &le;7 pieces: the theoretically correct outcome (Win/Draw/Loss) from Syzygy tablebases.</dd>
                </dl>

                <h3>Player Summary</h3>
                <dl class="glossary">
                    <dt>ACPL</dt>
                    <dd>Average Centipawn Loss across all moves. Lower = more accurate play. Typical values: &lt;20 excellent, &lt;40 good, &gt;80 poor.</dd>

                    <dt>ACPL Std Dev</dt>
                    <dd>Standard deviation of centipawn loss values. Lower values indicate more consistent play quality.</dd>

                    <dt>25-75 CPL Range</dt>
                    <dd>Interquartile range of centipawn losses, showing the typical range of move quality excluding extreme values.</dd>
                </dl>
            </div>
        </div>
    </section>

